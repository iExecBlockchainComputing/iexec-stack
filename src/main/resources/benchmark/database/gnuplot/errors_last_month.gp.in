set title "XWHEP : Error Jobs last month, per day\nTITLE"
set xlabel "Days"
set ylabel "Jobs"
set encoding iso_8859_1
set key outside
#set key 80, 535
#set key 1,3
#set size 1.3,1
set key spacing 1.5
set key width 1.5
#set logscale y
set xdata time
set timefmt "%Y/%m/%d"
set format x "%Y/%m/%d"
set xtics rotate



set terminal png
set output 'FILEOUT_errors_last_month.png'

plot 'FILEIN_errors_last_month.csv' using 1:2 with lines  lt 2  lw 2 title 'Errors'
